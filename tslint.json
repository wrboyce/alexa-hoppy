{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "align": false,
    "indent": false,
    "interface-name": [true, "never-prefix"],
    "no-console": [true, "log"],
    "one-variable-per-declaration": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
  },
  "rulesDirectory": []
}